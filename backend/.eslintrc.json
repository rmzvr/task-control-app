{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "indent": [
      "error",
      4
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": true
      }
    ]
  }
}